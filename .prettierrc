{
  "importOrder": [
      "^@/(.*)$",
      "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "printWidth": 180,
  "proseWrap": "always",
  "semi": false,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "tabWidth": 4,
  "trailingComma": "all",
  "arrowParens": "avoid"
}